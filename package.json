{
  "name": "synapse",
  "version": "1.3.0",
  "description": "Browser extension for FAST Token processing with ML-based user behavior prediction",
  "author": "Synapse Project Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/synapse"
  },
  "keywords": [
    "browser-extension",
    "machine-learning",
    "user-behavior",
    "tokenization",
    "sequence-prediction",
    "firefox"
  ],
  "scripts": {
    "build": "tsc && node build.js && web-ext build --overwrite-dest",
    "start": "web-ext run --target=firefox-desktop",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.0.0",
    "@types/webextension-polyfill": "^0.12.3",
    "typescript": "^5.9.2",
    "web-ext": "^7.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "webextension-polyfill": "^0.12.0"
  }
}
